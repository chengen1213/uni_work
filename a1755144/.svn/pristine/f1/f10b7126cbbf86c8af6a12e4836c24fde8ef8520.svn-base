
size(640, 360);
background(0);
stroke(153);
strokeWeight(4);
strokeCap(SQUARE);

int a = 50;
int b = 120;
int c = 180;

int x1;
int x2;
int y;

line(a, b, a+c, b);
line(a, b+10, a+c, b+10);
line(a, b+20, a+c, b+20);
line(a, b+30, a+c, b+30);

x1 = a+c+c; 
x2 = a+c+c+c; 

line(x1, b, x2, b);
line(x1, b+10, x2, b+10);
line(x1, b+20, x2, b+20);
line(x1, b+30, x2, b+30);

y = 100;

line(a+c, b+y, x1, b+y);
line(a+c, b+10+y, x1, b+10+y);
line(a+c, b+20+y, x1, b+20+y);
line(a+c, b+30+y, x1, b+30+y);